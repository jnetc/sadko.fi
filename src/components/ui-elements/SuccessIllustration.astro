  }

  .bold-path {
    stroke-dashoffset: 180;
    stroke-dasharray: 180;
    animation: bold 0.9s ease-in-out forwards;
  }

  .thin-path {
    stroke-dashoffset: 180;
    stroke-dasharray: 180;
    animation: thin 0.3s cubic-bezier(0.49, 0, 1, 0.4) forwards;
    animation-delay: 0.4s;
  }

  @keyframes circle {
    100% {
      stroke-dashoffset: 490;
      opacity: 1;
    }
  }

  @keyframes bold {
    100% {
      stroke-dashoffset: 100;
    }
  }

  @keyframes thin {
    100% {
      stroke-dashoffset: 100;
    }
  }
</style>
